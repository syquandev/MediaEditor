  "public_header_files": "SignalCoreKit/src/**/*.h",
  "prefix_header_file": "SignalCoreKit/SCKPrefix.h",
  "xcconfig": {
    "OTHER_CFLAGS": "$(inherited) -DSQLITE_HAS_CODEC"
  },
  "dependencies": {
    "CocoaLumberjack": [

    ]
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "source_files": "SignalCoreKitTests/src/**/*.{h,m,swift}"
    }
  ],
  "swift_version": "5.0"
}
